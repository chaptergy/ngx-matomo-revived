{
  "name": "ngx-matomo-revived",
  "version": "0.1.4",
  "author": "Arnaud Mouronval <arnaud.mouronval@gmail.com>, Chaptergy <26956711+chaptergy@users.noreply.github.com>",
  "description": "Matomo (aka. Piwik) web analytics for applications based on Angular 11.",
  "keywords": [
    "matomo",
    "piwik",
    "angular",
    "aot",
    "web analytics"
  ],
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/chaptergy/ngx-matomo-revived.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test-headless": "ng test --codeCoverage=true --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rimraf dist/*",
    "format:write": "prettier projects/**/*.{ts,json,md} **/README.md --write",
    "format:test": "prettier projects/**/*.{ts,json,md} **/README.md --list-different",
    "compodoc": "compodoc -c ./.compodocrc.json",
    "copy-license": "copyfiles ./LICENSE ./dist/ngx-matomo-revived",
    "copy-readme": "copyfiles ./README.md ./dist/ngx-matomo-revived",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "release": "standard-version",
    "postinstall": "ngcc"
  },
  "dependencies": {
    "@angular/animations": "^11.2.9",
    "@angular/common": "^11.2.9",
    "@angular/compiler": "^11.2.9",
    "@angular/core": "^11.2.9",
    "@angular/forms": "^11.2.9",
    "@angular/platform-browser": "^11.2.9",
    "@angular/platform-browser-dynamic": "^11.2.9",
    "@angular/router": "^11.2.9",
    "rxjs": "~6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.8",
    "@angular/cli": "^11.2.8",
    "@angular/compiler-cli": "^11.2.9",
    "@angular/language-service": "^11.2.9",
    "@types/jasmine": "^3.6.9",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "copyfiles": "^2.4.1",
    "eslint": "^7.24.0",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.3.2",
    "karma-coverage": "^2.0.3",
    "karma-firefox-launcher": "^2.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^11.2.4",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.0",
    "typescript": "^4.1.5"
  }
}
